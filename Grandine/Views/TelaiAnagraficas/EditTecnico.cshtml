@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    @Html.HiddenFor(model => model.IDCommessa)


    @Html.HiddenFor(model => model.InsertUser)
    @Html.HiddenFor(model => model.InsertDate)
    @Html.HiddenFor(model => model.NomeFile)
    @Html.HiddenFor(model => model.IDCommessa)

    @Html.HiddenFor(model => model.DataIn)
    @Html.HiddenFor(model => model.DataOut)
    @Html.HiddenFor(model => model.NFattAttiva)
    @Html.HiddenFor(model => model.DataFattAtt)
    @Html.HiddenFor(model => model.ImpFattAtt)

    @Html.HiddenFor(model => model.NumFattTecnico)
    @Html.HiddenFor(model => model.DataFatturaPassiva)
    @Html.HiddenFor(model => model.ImportoFattPass)
    @Html.HiddenFor(model => model.IDCarrozzeria1)
    @Html.HiddenFor(model => model.NumFattCarrozzeria1)
    @Html.HiddenFor(model => model.DataFatturaCarrozzeria1)
    @Html.HiddenFor(model => model.ImportoCarrozzeria1)
    @Html.HiddenFor(model => model.IDCarrozzeria2)
    @Html.HiddenFor(model => model.NumFattCarrozzeria2)
    @Html.HiddenFor(model => model.DataFatturaCarrozzeria2)
    @Html.HiddenFor(model => model.ImportoCarrozzeria2)
    @Html.HiddenFor(model => model.IDCarrozzeria3)
    @Html.HiddenFor(model => model.NumFattCarrozzeria3)
    @Html.HiddenFor(model => model.DataFatturaCarrozzeria3)
    @Html.HiddenFor(model => model.ImportoCarrozzeria3)
    @Html.HiddenFor(model => model.IDCarglass)
    @Html.HiddenFor(model => model.NumFattCarGlass)
    @Html.HiddenFor(model => model.ImportoFattCarGlass)
    @Html.HiddenFor(model => model.DataFatturaCarglass)
    @Html.HiddenFor(model => model.IDBisarchistaAndata)
    @Html.HiddenFor(model => model.NumFattBisarchistaA)
    @Html.HiddenFor(model => model.DataFattBisarchistaA)
    @Html.HiddenFor(model => model.CostoAndata)
    @Html.HiddenFor(model => model.IDBisarchistaRitorno)
    @Html.HiddenFor(model => model.NumFattBisarchistaR)
    @Html.HiddenFor(model => model.DataFattBisarchistaR)
    @Html.HiddenFor(model => model.CostoRitorno)
    @Html.HiddenFor(model => model.Costi)
    @Html.HiddenFor(model => model.Chiave)
    @Html.HiddenFor(model => model.Fila)
    @Html.HiddenFor(model => model.IsUrgente)


    <form>
        <div class="form-row">
            <div class="form-group col-md-3">
                @Html.LabelFor(model => model.Telaio, htmlAttributes: new { @class = "control-label col-md-3" })
                <div>
                    @Html.EditorFor(model => model.Telaio, new { htmlAttributes = new { @class = "form-control", @style = "height:20px" } })
                    @Html.ValidationMessageFor(model => model.Telaio, "", new { @class = "text-danger" })
                </div>
            </div>
            <div class="form-group col-md-3">
                @Html.LabelFor(model => model.Modello, htmlAttributes: new { @class = "control-label col-md-3" })
                <div>
                    @Html.EditorFor(model => model.Modello, new { htmlAttributes = new { @class = "form-control", @style = "height:20px" } })
                    @Html.ValidationMessageFor(model => model.Modello, "", new { @class = "text-danger" })
                </div>
            </div>
            <div class="form-group col-md-3">
                @Html.Label("Targa", htmlAttributes: new { @class = "control-label col-md-3" })
                <div>
                    @Html.EditorFor(model => model.Targa, new { htmlAttributes = new { @class = "form-control", @style = "height:20px" } })
                    @Html.ValidationMessageFor(model => model.Targa, "", new { @class = "text-danger" })
                </div>
            </div>
            <div class="form-group col-md-3">
                @Html.Label("Tecnico", htmlAttributes: new { @class = "control-label col-md-3" })
                <div>
                    @Html.DropDownList("IDTecnico", null, htmlAttributes: new { @class = "form-control", @style = "height:35px" })
                    @Html.ValidationMessageFor(model => model.IDTecnico, "", new { @class = "text-danger" })
                </div>
            </div>
        </div>

        <div class="form-row">
            <div class="form-group col-md-12">
                @Html.Label("Gravita", htmlAttributes: new { @class = "control-label col-md-12" })
                <div>
                    @Html.DropDownList("IDGravita", null, htmlAttributes: new { @class = "form-control", @style = "height:35px" })
                    @Html.ValidationMessageFor(model => model.IDGravita, "", new { @class = "text-danger" })

                </div>
            </div>
        </div>

        <br /><br />


        <div class="form-row">
            <div class="form-group col-md-12">
                @Html.LabelFor(model => model.Annotazioni, htmlAttributes: new { @class = "control-label col-md-12" })
                @*<div>*@
                @Html.TextAreaFor(model => model.Annotazioni, new { htmlAttributes = new { @class = "form-control input-lg" } })
                @Html.ValidationMessageFor(model => model.Annotazioni, "", new { @class = "text-danger" })
                @*</div>*@
            </div>
        </div>

        <div class="form-row">
            <div class="form-group col-md-12">
                <input type="submit" value="Salva dati" class="btn btn-success" />
                @Html.ActionLink("Torna a lista", "Index", new { IDCommessa = Session["IDCommessa"] }, new { @class = "btn btn-primary" })
                @Html.ActionLink("Ricambi", "Edit", "Ricambi", new { IDTelaio = Model.ID }, new { @class = "btn btn-default" })
            </div>
        </div>


    </form>
}



